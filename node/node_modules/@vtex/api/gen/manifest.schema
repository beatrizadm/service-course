        },
        "description": {
            "type": "string"
        },
        "mustUpdateAt": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "peerDependencies": {
            "additionalProperties": {
                "type": "string"
            },
            "type": "object"
        },
