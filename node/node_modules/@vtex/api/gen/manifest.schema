                "plans": {
                    "items": {
                        "$ref": "#/definitions/Plan"
                    },
                    "type": "array"
                },
                "support": {
                    "$ref": "#/definitions/Support"
                },
                "type": {
                    "$ref": "#/definitions/BILLING_TYPE"
                }
            },
            "type": "object"
        },
        "Plan": {
            "properties": {
                "currency": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "price": {
                    "$ref": "#/definitions/Price"
                }
            },
            "type": "object"
        },
        "Policy": {
            "properties": {
                "attrs": {
                    "additionalProperties": {
                        "type": "string"
                    },
                    "type": "object"
                },
                "name": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "Price": {
            "properties": {
