            "items": {
                "$ref": "#/definitions/Policy"
            },
            "type": "array"
        },
        "registries": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "settingsSchema": {
        },
        "title": {
            "type": "string"
