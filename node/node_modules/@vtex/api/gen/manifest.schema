                "inclusiveTo": {
                    "type": "number"
                },
                "multiplier": {
                    "type": "number"
                }
            },
            "type": "object"
        },
        "Support": {
            "properties": {
                "email": {
                    "type": "string"
                },
                "phone": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
